<header class="bg-yellow-200 p-4">
  <div class="container mx-auto flex justify-between items-center">
    <!-- Logo -->
    <div class="text-2xl font-bold text-gray-800">
      <a href="/">Logo Here</a>
    </div>

    <!-- Navigation -->
    <nav>
      <ul class="flex space-x-6">
        <li>
          <a href="#" class="text-gray-700 hover:text-gray-900">Our Vision</a>
        </li>
        <li>
          <a href="/likes" class="text-gray-700 hover:text-gray-900">Likes</a>
        </li>
        <li>
          <a href="/cart" class="text-gray-700 hover:text-gray-900">Cart</a>
        </li>
        <% if (userdata) { %>
        <li>
          <a href="/profile" class="text-gray-700 hover:text-gray-900">
            <%=userdata.username %>
          </a>
        </li>

        <% } else { %>
        <li>
          <a href="/login" class="text-gray-700 hover:text-gray-900"
            >Sign In or Register</a
          >
        </li>
        <% } %>
      </ul>
    </nav>
  </div>
</header>
