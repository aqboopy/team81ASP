<h1>User List</h1>
<ul>
	<% users.forEach(user => { %>
	<li><%= user.name %> - <%= user.email %></li>
	<% }); %>
</ul>

<h2>Add User</h2>
<form action="/users/add" method="POST">
	<input type="text" name="name" placeholder="Name" required />
	<input type="email" name="email" placeholder="Email" required />
	<button type="submit">Add User</button>
</form>
